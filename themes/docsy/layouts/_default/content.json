[
    {{ with .Params.product }}
        {{ range . }}
            {
                "time": "{{ .time }}",
                "title": "{{ .title }}",
                "content": {{ .content | jsonify }} ,
                {{ if .url }}
                "url": "{{ $.Param "domain" }}{{ .url }}"
                {{ end }}
            },
        {{ end }}
    {{ end }}       
]